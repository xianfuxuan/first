<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="gamegame">

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name="gamegame.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity android:name=".twoballs.TwoBallsActivity"></activity>
        <activity android:label="snake" android:name=".snake.SnakeActivity"></activity>
        <activity android:label="plane" android:name=".plane.PlaneActivity"></activity>
        <activity android:label="mirage" android:name=".mirage.MirageActivity"></activity>
    </application>

</manifest>